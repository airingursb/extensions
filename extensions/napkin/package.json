{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "send-to-napkin",
  "title": "Send to Napkin",
  "description": "Send your thoughts to Napkin",
  "icon": "command-icon.png",
  "author": "airingursb",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "send-to-napkin",
      "title": "Send Thoughts",
      "subtitle": "Napkin",
      "description": "Send your thoughts to Napkin",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "email",
      "title": "Napkin email",
      "description": "Your Napkin email",
      "type": "textfield",
      "required": true
    },
    {
      "name": "api",
      "title": "Napkin API Token",
      "description": "Your Napkin API Token",
      "type": "textfield",
      "required": true
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.38.3",
    "axios": "^0.24.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
